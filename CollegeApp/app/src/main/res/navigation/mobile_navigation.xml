<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.ashfaq.collegeapp.ui.home.HomeFragment"
        android:label="Home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_ebook"
        android:name="com.ashfaq.collegeapp.ui.ebook.EbookFragment"
        android:label="Ebook"
        tools:layout="@layout/fragment_ebook" />

    <fragment
        android:id="@+id/navigation_faculty"
        android:name="com.ashfaq.collegeapp.ui.faculty.FacultyFragment"
        android:label="Faculty"
        tools:layout="@layout/fragment_faculty" />

    <fragment
        android:id="@+id/navigation_gallery"
        android:name="com.ashfaq.collegeapp.ui.gallery.GalleryFragment"
        android:label="Gallery"
        tools:layout="@layout/fragment_gallery" />

    <fragment
        android:id="@+id/navigation_notice"
        android:name="com.ashfaq.collegeapp.ui.notice.NoticeFragment"
        android:label="Notice"
        tools:layout="@layout/fragment_notice" />
</navigation>